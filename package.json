{
  "name": "react-redux-quiz",
  "version": "1.0.0",
  "description": "Quiz game web app",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "NODE_ENV=production npm run clean && webpack -p",
    "start": "webpack-dev-server",
    "deploy": "npm run build && gh-pages -d dist",
    "deploy-lambda": "serverless deploy -v --stage dev",
    "update-lambda": "serverless deploy function -f funcOne --stage dev"
